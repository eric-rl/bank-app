<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane alignment="center" hgap="10" prefHeight="600.0" prefWidth="800.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.account.AccountController">
   <children>
      <ScrollPane prefHeight="400.0" prefWidth="600.0" GridPane.rowIndex="5">
         <content>
            <VBox fx:id="transactionBox" maxHeight="600.0" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
         </content>
      </ScrollPane>
      <Button fx:id="goToHomeButton" mnemonicParsing="false" onAction="#goToHomeController" text="Gå tillbaka" />
      <Button fx:id="pay" mnemonicParsing="false" onAction="#goToTransactionController" text="Betala med konto" GridPane.rowIndex="2" />
      <Button fx:id="generateMore" mnemonicParsing="false" onAction="#generateMore" text="Visa alla transaktioner" GridPane.rowIndex="1" />
      <Button fx:id="cardPayment" mnemonicParsing="false" onAction="#cardPayment" text="Betala 200kr med kort" visible="false" GridPane.rowIndex="3" />
      <Button fx:id="salaryIncome" mnemonicParsing="false" onAction="#setSalaryIncome" text="Få lön" visible="false" GridPane.rowIndex="4" />

   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints />
   </rowConstraints>
</GridPane>
